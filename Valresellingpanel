  <script>
    const validKeys = [
      'fortnite-10433218', 'fortnite-19600133', 'fortnite-89083863', 'fortnite-79402654', 'fortnite-23511615',
      'temp-59407816', 'temp-18495931', 'temp-03413164', 'temp-75255341', 'temp-92832764',
      'perm-83503056', 'perm-41395376', 'perm-72423884', 'perm-96965328', 'perm-71012269',
      'add-16697848', 'add-01845146', 'add-27048281', 'add-48932528', 'add-80957015',
      'KEY-4K9P2M', 'KEY-7J5Q8L', 'KEY-2X1N3B', 'KEY-9F6R4T', 'KEY-5H3Y7U',
      'KEY-1C8V2J', 'KEY-6D4W9K', 'KEY-3G7Z5P', 'KEY-8E2M1Q', 'KEY-4A9N6R',
      'KEY-7B5L3T', 'KEY-2F8J9U', 'KEY-6C1Y4W', 'KEY-3D7K2P', 'KEY-9G4M8Q',
      'KEY-5E2R1T', 'KEY-1A8N6J', 'KEY-4B5L3V', 'KEY-7C9Y2W', 'KEY-3F6K8P',
      'KEY-9D2M1Q', 'KEY-5G4R7T', 'KEY-1E8J3U', 'KEY-4H6N2W', 'KEY-7A9L5P',
      'KEY-2B3Y8Q', 'KEY-6C5M1T', 'KEY-3D9R4J', 'KEY-8F2K7U', 'KEY-4G6N1W',
      'KEY-7E9L3P', 'KEY-2A5Y8Q', 'KEY-6B3M1T', 'KEY-9D4R7J', 'KEY-5F2K8U',
      'KEY-1G6N3W', 'KEY-4E9L5P', 'KEY-7H2Y8Q', 'KEY-3A5M1T', 'KEY-6B9R4J'
    ];

    function checkKey() {
      const input = document.getElementById('keyInput').value;
      const resellPanel = document.getElementById('resellPanel');
      const resellMessage = document.getElementById('resellMessage');
      if (validKeys.includes(input)) {
        resellPanel.classList.add('active');
        resellMessage.textContent = 'Access granted! Enjoy the Resell Panel.';
        resellMessage.className = 'success';
      } else {
        resellMessage.textContent = 'Invalid key. Please try again.';
        resellMessage.className = 'error';
      }
    }

    function generateKeys(type) {
      const numKeys = 5; // Generate 5 new keys per click
      const keys = [];
      for (let i = 0; i < numKeys; i++) {
        const randomNum = Math.floor(10000000 + Math.random() * 90000000);
        keys.push(`${type}-${randomNum}`);
      }
      const outputId = `${type}Keys`;
      document.getElementById(outputId).textContent = `New Keys: ${keys.join(', ')}`;
      validKeys.push(...keys); // Add new keys to the valid keys list
    }
  </script>
